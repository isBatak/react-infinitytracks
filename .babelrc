{
  "presets": ["env", "react", "stage-0"],
  "plugins": [
    "transform-async-to-generator",
    "transform-decorators-legacy",
    "transform-class-properties",
    [
      "module-resolver", {
        "extensions": ["", ".ts", ".tsx", ".js", ".jsx"],
        "alias": {
          "react-infinitytracks": "./src"
        }
      }
    ]
  ]
}
